rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /referrals/{referralId} {
      allow read, write: if true;
    }
    match /users/{userId} {
      allow read, write: if true;
    }
    match /partners/{partnerId} {
      allow read, write: if true;
    }
  }
}
